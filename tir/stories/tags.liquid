---
layout: storyLayout.liquid
pagination:
  data: collections.storyTags
  size: 1
  alias: tagName
  addAllPagesToCollections: true
permalink: "stories/tag/{{ tagName | slug }}/"
eleventyNavigation:
  parent: "story-tags"
eleventyComputed:
  displayTitle: "Tag: {{ tagName }}"
  title: "{{ tagName }}"
  key: "stories-{{ tagName }}"
---
{% assign filteredStories = collections.stories | filterStoriesByTag: tagName %}
{% include 'storylist', stories: filteredStories %}