---
permalink: "fun/fav/data.js"
eleventyExcludeFromCollections: true
---
const data = [
{% for ch in characters %}
{% assign profile = ch.name | lowerCase | append: ' profile' %}{% assign thumb = ch.name | lowerCase | append: ' thumb' %}{% capture url %}{% imageUrl profile 500 'webp' thumb %}{% endcapture %}
{
name: "{{ ch.name }}",
color: "{{ ch.color }}",
desc: "{{ ch.desc }}",
img: "{{ url | htmlBaseUrl }}"
},
{% endfor %}
];