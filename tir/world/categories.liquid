---
layout: article.liquid
pagination:
  data: collections.worldCats
  size: 1
  alias: tagName
  addAllPagesToCollections: true
permalink: "world/category/{{ tagName | slug }}/"
eleventyNavigation:
  parent: Categories
eleventyComputed:
  title: "{{ tagName }}"
---
{% assign filteredFiles = collections.world | filterWorldByTag: tagName %}
<ul>
    {% for file in filteredFiles %}
        {% include 'fileLi', file: file %}
    {% endfor %}
</ul>
